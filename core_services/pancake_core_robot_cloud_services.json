{
  "Name": "Services/RobotCloud",
  "Services": [
    {
      "Type": "CloudProxy",
      "Settings": {
        "Alias": "Robot/cloud",
        "DefaultRunning": "true",
        "CreateSession": "true",
        "PingPongRetries":2
      }
    },
    {
      "Type": "RTPReceiver",
      "Settings": {
        "Alias": "Robot/receiver/data/user/user1",
        "DefaultRunning": "false",
        "PeerIP": "127.0.0.1",
        "Data": [
          {
            "ID": "11",
            "Name": "Data",
            "RTPPort": "10501",
            "Sink": {
              "Type": "shmem",
              "Resource": "user-1-data",
              "Size": "4096"
            }
          }
        ]
      }
    },
    {
      "Type": "RTPReceiver",
      "Settings": {
        "Alias": "Robot/receiver/data/user/user2",
        "DefaultRunning": "false",
        "PeerIP": "127.0.0.1",
        "Data": [
          {
            "ID": "12",
            "Name": "Data",
            "RTPPort": "10502",
            "Sink": {
              "Type": "shmem",
              "Resource": "user-2-data",
              "Size": "4096"
            }
          }
        ]
      }
    },
    {
      "Type": "RTPSender",
      "Settings": {
        "Alias": "Robot/sender/data/feedback/user",
        "DefaultRunning": "false",
        "PeerIP": "127.0.0.1",
        "Data": [
          {
            "ID": "13",
            "Name": "Data",
            "Reliable": "false",
            "RTPPort": "10601",
            "Source": {
              "Type": "shmem",
              "Resource": "feedback-to-user"
            }
          }
        ]
      }
    },
    {
      "Type": "RTPSender",
      "Settings": {
        "Alias": "User/sender/video/micro",
        "DefaultRunning": "true",
        "PeerIP": "127.0.0.1",
        "RTCP": "false",
        "Video": [
          {
            "ID": "21",
            "Name": "MicroCamera",
            "RTPPort": "10701",
            "Source": {
              "Type": "shmem",
              "Resource": "micro-camera"
            },
            "Encoder": {
              "Encoder": "x264",
              "Bitrate": "1000",
              "Quality": "7"
            }
          }
        ]
      }
    },
    {
      "Type": "RTPSender",
      "Settings": {
        "Alias": "User/sender/video/fruit",
        "DefaultRunning": "true",
        "PeerIP": "127.0.0.1",
        "RTCP": "false",
        "Video": [
          {
            "ID": "22",
            "Name": "FruitCamera",
            "RTPPort": "10702",
            "Source": {
              "Type": "shmem",
              "Resource": "fruit-camera"
            },
            "Encoder": {
              "Encoder": "x264",
              "Bitrate": "1000",
              "Quality": "7"
            }
          }
        ]
      }
    },
    {
      "Type": "RTPSender",
      "Settings": {
        "Alias": "User/sender/video/cake",
        "DefaultRunning": "true",
        "PeerIP": "127.0.0.1",
        "RTCP": "false",
        "Video": [
          {
            "ID": "23",
            "Name": "CakeCamera",
            "RTPPort": "10703",
            "Source": {
              "Type": "shmem",
              "Resource": "cake-camera"
            },
            "Encoder": {
              "Encoder": "x264",
              "Bitrate": "1000",
              "Quality": "7"
            }
          }
        ]
      }
    }
  ]
}
