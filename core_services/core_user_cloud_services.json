{
  "Name": "Services/UserCloud",
  "Services": [
    {
      "Type": "CloudProxy",
      "Settings": {
        "Alias": "User/cloud",
        "CreateSession": "false",
        "DefaultRunning": "true"
      }
    },
    {
      "Type": "RTPSender",
      "Settings": {
        "Alias": "User/sender/data/user/user1",
        "DefaultRunning": "false",
        "PeerIP": "127.0.0.1",
        "Data": [
          {
            "ID": "11",
            "Name": "Data",
            "Reliable": "false",
            "RTPPort": "10501",
            "Source": {
              "Type": "shmem",
              "Resource": "user-1-data"
            }
          }
        ]
      }
    },
    {
      "Type": "RTPReceiver",
      "Settings": {
        "Alias": "User/receiver/data/feedback/user",
        "DefaultRunning": "false",
        "PeerIP": "127.0.0.1",
        "Data": [
          {
            "ID": "13",
            "Name": "Data",
            "RTPPort": "10601",
            "Sink": {
              "Type": "shmem",
              "Resource": "feedback-to-user",
              "Size": "4096"
            }
          }
        ]
      }
    },
    {
      "Type": "RTPReceiver",
      "Settings": {
        "Alias": "User/receiver/video/micro",
        "DefaultRunning": "false",
        "PeerIP": "127.0.0.1",
        "Secured": "false",
        "RTCP": "false",
        "FEC": "true",
        "Video": [
          {
            "ID": "21",
            "Name": "RobotCamera",
            "RTPPort": "10701",
            "Type": "x264",
            "Sink": {
              "Type": "shmem",
              "Resource": "robot-camera",
              "Size": "6220800"
            }
          }
        ]
      }
    },
    {
      "Type": "VideoFormat",
      "Settings": {
        "Alias": "User/video/robot/micro/RGB",
        "DefaultRunning": "true",
        "Source": {
          "Type": "shmem",
          "Resource": "robot-camera"
        },
        "Sink": {
          "Type": "shmem",
          "Format": "RGB",
          "Resource": "robot-camera-RGB",
          "size": "6220800"
        }
      }
    }
  ]
}
